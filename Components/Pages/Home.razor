@inject ApplicationDbContext DbContext

@page "/"
@using GymSync.Data
@using GymSync.Models
@using Microsoft.EntityFrameworkCore;



<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<h3> Users List </h3>
<ul>
	@foreach (var equipment in equipmentlist)
	{
		<li>@equipment.equipment_id @equipment.in_use @equipment.location_name</li>
				
	}
</ul>

@code{
	private List<EquipmentEntity> equipmentlist = new();

	protected override async Task OnInitializedAsync()
	{
		equipmentlist = await DbContext.EQUIPMENT.ToListAsync();	
	}
}
